<svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Two-storey house design -->
  <!-- Foundation -->
  <rect x="2" y="18" width="20" height="4" fill="currentColor" opacity="0.3"/>
  
  <!-- First floor -->
  <rect x="3" y="12" width="18" height="6" fill="currentColor" stroke="currentColor" stroke-width="0.5"/>
  
  <!-- Second floor -->
  <rect x="5" y="6" width="14" height="6" fill="currentColor" stroke="currentColor" stroke-width="0.5"/>
  
  <!-- Roof for second floor -->
  <polygon points="4,6 12,2 20,6" fill="currentColor" opacity="0.8"/>
  
  <!-- Roof for first floor -->
  <polygon points="2,12 4,10 20,10 22,12" fill="currentColor" opacity="0.6"/>
  
  <!-- Windows - First floor -->
  <rect x="6" y="14" width="2" height="2" fill="white" opacity="0.8"/>
  <rect x="10" y="14" width="2" height="2" fill="white" opacity="0.8"/>
  <rect x="14" y="14" width="2" height="2" fill="white" opacity="0.8"/>
  <rect x="18" y="14" width="2" height="2" fill="white" opacity="0.8"/>
  
  <!-- Windows - Second floor -->
  <rect x="7" y="8" width="2" height="2" fill="white" opacity="0.8"/>
  <rect x="11" y="8" width="2" height="2" fill="white" opacity="0.8"/>
  <rect x="15" y="8" width="2" height="2" fill="white" opacity="0.8"/>
  
  <!-- Door -->
  <rect x="11" y="15" width="2" height="3" fill="white" opacity="0.9"/>
  <circle cx="12.5" cy="16.5" r="0.2" fill="currentColor"/>
  
  <!-- Chimney -->
  <rect x="16" y="3" width="1.5" height="4" fill="currentColor" opacity="0.7"/>
</svg>